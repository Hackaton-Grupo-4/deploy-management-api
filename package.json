{
  "name": "deploy.management.api",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "start": "node dist/main/server.js",
    "check": "npm-check -u -s",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "dev": "npm run build && docker-compose up",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "watch": "tsc -p tsconfig-build.json -w",
    "stop": "docker-compose down",
    "clean": "docker container prune -f",
    "clean:volumes": "docker volume prune -f",
    "migration:run": "prisma migrate deploy && npm run db:generate",
    "migration:generate": "prisma migrate dev --create-only --name",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "test": "jest --passWithNoTests --silent --noStackTrace --no-cache --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "git-commit-msg-linter": "^4.1.1",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^12.0.2",
    "mockdate": "^3.0.5",
    "prisma": "^3.8.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@prisma/client": "3.8.1",
    "@types/express": "^4.17.13",
    "axios": "^0.26.0",
    "express": "^4.17.2",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.15",
    "swagger-ui-express": "^4.3.0"
  },
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}